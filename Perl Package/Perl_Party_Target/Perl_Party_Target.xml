						</Size>
						<Anchors>
							<Anchor point="TOPLEFT">
								<Offset>
									<AbsDimension x="0" y="0"/>
								</Offset>
							</Anchor>
						</Anchors>
						<Scripts>
							<OnLoad>
								self:RegisterForClicks("AnyUp");
								self:RegisterForDrag("LeftButton");
								Perl_Party_Target_CastClickOverlay_OnLoad(self);
							</OnLoad>
							<OnDragStart>
								Perl_Party_Target_DragStart(self, arg1);
							</OnDragStart>
							<OnDragStop>
								Perl_Party_Target_DragStop(self, arg1);
							</OnDragStop>
							<OnEnter>
								UnitFrame_OnEnter(self:GetParent():GetParent());
							</OnEnter>
							<OnLeave>
								UnitFrame_OnLeave();
							</OnLeave>
						</Scripts>
					</Button>
				</Frames>
