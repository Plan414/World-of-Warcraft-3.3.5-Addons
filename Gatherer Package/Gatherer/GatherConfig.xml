			<OnLoad>
				this:SetFrameLevel( this:GetFrameLevel() + 4 );
			</OnLoad>
			<OnClick>
				Gatherer.Config.Blacklist_IgnoreButton_OnClick(self)
			</OnClick>
		</Scripts>
		<HighlightTexture file="Interface\QuestFrame\UI-QuestTitleHighlight" alphaMode="ADD"/>
	</Button>

