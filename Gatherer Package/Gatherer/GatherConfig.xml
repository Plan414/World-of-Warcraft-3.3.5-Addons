	<Button name="BlacklistFrameListLineTemplate" virtual="true">
		<Size>
			<AbsDimension x="260" y="16"/>
		</Size>
		<ButtonText inherits="GameFontNormal" justifyH="LEFT" setAllPoints="true"/>
