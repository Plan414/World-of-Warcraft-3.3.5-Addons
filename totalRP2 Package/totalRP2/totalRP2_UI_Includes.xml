			<Anchor point="CENTER"/>
		</Anchors>
		<Frames>
			<Button name="$parentPlusOne" inherits="TRP2_HelpButtonTemplate">
				<Size x="15" y="15"/>
				<NormalTexture file="Interface\Minimap\UI-Minimap-ZoomInButton-Up"/>
				<PushedTexture file="Interface\Minimap\UI-Minimap-ZoomInButton-Down"/>
